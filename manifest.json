{
  "name": "Russian Doomer",
  "description": "Soviet vibes",
  "developer": {
    "name": "Frédéric Bégin"
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 3,
  "mod": {
    "license": "license.txt",
    "payload": {
      "background_music": ["sounds/background_music.mp3"],
      "keyboard_sounds": {
        "TYPING_LETTER": ["sounds/keyboard.mp3"],
        "TYPING_ENTER": ["sounds/keyboard_fast.mp3"],
        "TYPING_SPACE": ["sounds/keyboard_fast.mp3"],
        "TYPING_BACKSPACE": ["sounds/keyboard_fast.mp3"]
      },
      "browser_sounds": {
        "SWITCH_TOGGLE": ["sounds/click.mp3"],
        "TAB_CLOSE": ["sounds/closetab.mp3"],
        "TAB_INSERT": ["sounds/lighter.mp3"],
        "HOVER": ["sounds/lighter.mp3"],
        "HOVER_UP": ["sounds/lighter.mp3"],
        "CLICK": ["sounds/importantclick.mp3"],
        "LIMITER_ON": ["sounds/click.mp3"],
        "LIMITER_OFF": ["sounds/click.mp3"],
        "IMPORTANT_CLICK": ["sounds/importantclick.mp3"],
        "FEATURE_SWITCH_ON": ["sounds/click.mp3"],
        "FEATURE_SWITCH_OFF": ["sounds/click.mp3"],
        "LEVEL_UPGRADE": ["sounds/lighter.mp3"],
        "TAB_SLASH": ["sounds/closetab.mp3"]
      },
      "shaders": {
        "Broken CRT TV": { "path": "shader/broken_crt.txt" },
        "Depression": { "path": "shader/depression.txt" }
      },
      "theme": {
        "dark": {
          "gx_accent": { "h": 211, "s": 70, "l": 40 },
          "gx_secondary_base": { "h": 211, "s": 29, "l": 19 }
        },
        "light": {
          "gx_accent": { "h": 112, "s": 80, "l": 44 },
          "gx_secondary_base": { "h": 272, "s": 32, "l": 16 }
        }
      },
      "wallpaper": {
        "dark": {
          "image": "wallpaper/soviet.png",
          "text_color": "#FFFFFF",
          "text_shadow": "#757575"
        },
        "light": {
          "image": "wallpaper/soviet.png",
          "text_color": "#FFFFFF",
          "text_shadow": "#0B000E"
        }
      }
    },
    "schema_version": 1
  },
  "version": "1.0"
}
